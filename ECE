This is the Electronics and Communication Engineering department file.
Contains information about ECE department and its laboratory facilities.

// Example of a switch case in C++ for ECE students
#include <iostream>
using namespace std;

int main() {
    int lab_num;
    cout << "Enter ECE lab number (1-4): ";
    cin >> lab_num;
    
    switch(lab_num) {
        case 1:
            cout << "ECE101: Basic Electronics Lab" << endl;
            break;
        case 2:
            cout << "ECE202: Digital Signal Processing Lab" << endl;
            break;
        case 3:
            cout << "ECE303: Microprocessors Lab" << endl;
            break;
        case 4:
            cout << "ECE404: Communication Systems Lab" << endl;
            break;
        default:
            cout << "Invalid lab number" << endl;
    }
    
    return 0;
} 